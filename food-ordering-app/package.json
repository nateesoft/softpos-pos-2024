{
  "name": "food-ordering-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.2",
    "@mui/material": "^6.4.2",
    "env-cmd": "^10.1.0",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.0",
    "react-router-dom": "^7.1.5",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.8.1",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "env-cmd -f ./.env react-scripts start",
    "local": "env-cmd -f ./.env.local react-scripts start",
    "build": "react-scripts build",
    "build:orderapp1": "env-cmd -f ./.env.orderapp1 react-scripts build && mv build orderapp1",
    "build:orderapp2": "env-cmd -f ./.env.orderapp2 react-scripts build && mv build orderapp2",
    "build:orderapp3": "env-cmd -f ./.env.orderapp3 react-scripts build && mv build orderapp3",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
