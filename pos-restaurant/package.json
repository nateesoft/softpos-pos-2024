{
  "name": "pos-restaurant",
  "version": "0.1.0",
  "private": true,
  "homepage": "pos-restaurant",
  "main": "electron.js",
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^6.1.6",
    "@mui/lab": "^6.0.0-beta.14",
    "@mui/material": "^6.1.6",
    "@mui/styled-engine-sc": "^6.1.8",
    "@mui/x-charts": "^7.22.3",
    "@mui/x-data-grid": "^7.23.0",
    "@mui/x-data-grid-generator": "^7.23.0",
    "@mui/x-date-pickers": "^7.23.3",
    "@xyflow/react": "^12.3.4",
    "axios": "^1.7.7",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "framer-motion": "^11.18.2",
    "http-proxy-middleware": "^3.0.3",
    "i18next": "^23.16.4",
    "js-cookie": "^3.0.5",
    "localtunnel": "^2.0.2",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-cookie": "^7.2.2",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.1.0",
    "react-idle-timer": "^5.7.2",
    "react-input-mask": "^2.0.4",
    "react-moment": "^1.1.3",
    "react-number-format": "^5.4.2",
    "react-pageflip": "^2.0.3",
    "react-router-dom": "^6.27.0",
    "react-scripts": "5.0.1",
    "react-simple-keyboard": "^3.8.51",
    "react-to-print": "^3.0.2",
    "react-trello": "^2.2.11",
    "socket.io-client": "^4.8.1",
    "string-similarity": "^4.0.4",
    "styled-components": "^6.1.13",
    "uuid": "^11.0.5",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "concurrently": "^9.1.2",
    "electron": "^33.4.2",
    "electron-builder": "^25.1.8",
    "electron-is-dev": "^3.0.1",
    "electron-packager": "^17.1.2",
    "env-cmd": "^10.1.0",
    "wait-on": "^8.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "local": "react-scripts start",
    "prod": "env-cmd -f ./.env.prod react-scripts start",
    "start:webapp": "env-cmd -f ./.env.webapp react-scripts start",
    "build": "react-scripts build",
    "build:local": "env-cmd -f ./.env.local react-scripts build",
    "build:prod": "env-cmd -f ./.env.prod react-scripts build",
    "build:webapp": "rm -rf build && rm -rf server/webapp && env-cmd -f ./.env.webapp react-scripts build && mv build server/webapp",
    "test": "react-scripts test",
    "start:electron": "electron .",
    "build:electron": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "softpos.restaurant-app",
    "productName": "RestuarantApp",
    "files": [
      "build/**/*",
      "public/electron.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
